<Window x:Class="Projekt.Okna.Klienci"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt.Okna"
        mc:Ignorable="d"
        Title="Klienci" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DataGridKli" HorizontalAlignment="Left" Height="177" Margin="10,10,0,0" VerticalAlignment="Top" Width="774" Loaded="DataGridKli_Loaded" SelectionChanged="DataGridKli_SelectionChanged"/>
        <Button x:Name="dodaj" Content="Dodaj" HorizontalAlignment="Left" Margin="80,302,0,0" VerticalAlignment="Top" Width="140" Height="50" Click="dodaj_Click"/>
        <Button x:Name="zapisz" Content="Zapisz" HorizontalAlignment="Left" Margin="337,302,0,0" VerticalAlignment="Top" Width="140" Height="50" Click="zapisz_Click"/>
        <Button x:Name="usun" Content="Usuń" HorizontalAlignment="Left" Margin="576,302,0,0" VerticalAlignment="Top" Width="140" Height="50" Click="usun_Click"/>

    </Grid>
</Window>
